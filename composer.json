{
    "name": "cihuyy/pbl-perpus-cihuyyy",
    "description": "PBL Sistem Peminjaman Ruangan",
    "type": "library",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\Controllers\\" : "app/controllers/",
            "App\\Models\\" : "app/models/",
            "App\\Core\\" : "app/core/",
            "App\\Error\\" : "app/error/",
            "App\\Utils\\" : "app/utils/",
            "App\\Components\\" : "app/components/"
        }
    },
    "scripts": {
        "migrate" : "php app/utils/Migrate.php",
        "seed" : "php app/utils/Seed.php"
    },
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "nesbot/carbon": "^3.10",
        "phpoffice/phpspreadsheet": "^5.3",
        "phpmailer/phpmailer": "^7.0"
    }
}
